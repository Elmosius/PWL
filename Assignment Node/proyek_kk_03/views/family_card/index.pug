extends ../layouts/master
block content
    div.content-header
        div.container-fluid
            div.row.mb-2
                div.col-sm-6
                    h1.m-0 Starter Page
                div.col-sm-6
                    ol.breadcrumb.float-sm-right
                        li.breadcrumb-item
                            a(href="#") Home
                        li.breadcrumb-item.active Starter Page
    div.content
        div.container-fluid
            div.card
                div.card-body
                   div.table-responsive
                       table.table.table-striped(id="table-fam-card")
                           thead
                                tr
                                    th Id
                                    th name
                           tbody
                                each famCard in famCards
                                    tr
                                        td #{famCard.id}
                                        td #{famCard.name}
